    "@types/sinon": "^5.0.5",
    "awesome-typescript-loader": "^5.2.1",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^4.5.2",
    "dojo-typings": "^1.11.9",
    "express": "^4.16.3",
    "jsdom": "^12.2.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.3",
    "node-sass-import-once": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.0.1",
    "sinon": "^6.3.5",
    "source-map-loader": "^0.2.4",
    "ts-node": "^7.0.1",
